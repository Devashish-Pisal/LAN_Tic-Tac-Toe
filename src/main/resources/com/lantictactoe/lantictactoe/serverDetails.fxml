<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="213.0" prefWidth="457.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lantictactoe.lantictactoe.Controllers.ServerDetailsController">
   <children>
      <Text fx:id="enterServerIPLabel" layoutX="40.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Server IP :">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fx:id="portNumberLabel" layoutX="45.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Port Number :">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <TextField fx:id="serverIPTextField" disable="true" layoutX="183.0" layoutY="33.0" prefHeight="26.0" prefWidth="194.0" promptText="localhost">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></TextField>
      <TextField fx:id="portNumberTextField" layoutX="183.0" layoutY="85.0" prefHeight="26.0" prefWidth="194.0" onMouseClicked="#clearWarningLabel"/>
      <Button fx:id="enterButton" layoutX="247.0" layoutY="165.0" mnemonicParsing="false" onAction="#onEnterButtonClick" prefHeight="33.0" prefWidth="65.0" text="Enter">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="warningLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="147.0" layoutY="124.0" prefHeight="26.0" prefWidth="267.0" textFill="#d41b1b">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
