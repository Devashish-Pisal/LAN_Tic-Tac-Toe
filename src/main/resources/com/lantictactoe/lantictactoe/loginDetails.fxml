<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="459.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lantictactoe.lantictactoe.Controllers.LoginDetailsController">
   <children>
      <Label fx:id="usernameLabel" layoutX="77.0" layoutY="62.0" text=" Username :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="84.0" layoutY="113.0" text="Password :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="208.0" layoutY="60.0" prefHeight="26.0" prefWidth="167.0" onMouseClicked="#clearWarningLabel"/>
      <TextField fx:id="passwordTextField" layoutX="208.0" layoutY="112.0" prefHeight="26.0" prefWidth="167.0" onMouseClicked="#clearWarningLabel"/>
      <Button fx:id="enterButton" layoutX="311.0" layoutY="191.0" mnemonicParsing="false" onAction="#onEnterButtonClick" prefHeight="36.0" prefWidth="63.0" text="Enter">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="warningLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="148.0" prefHeight="36.0" prefWidth="459.0" textFill="#ea0d0d">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Button fx:id="registerButton" layoutX="208.0" layoutY="191.0" mnemonicParsing="false" onAction="#onRegisterButtonClick" prefHeight="36.0" prefWidth="78.0" text="Register" >
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
